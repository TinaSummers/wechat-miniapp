.mask-wrapper .content-area.barcode .card-box .card-num {
  font-size: 28rpx;
  color: #fff;
  position: absolute;
  bottom: 48rpx;
  left: 40rpx;
}
.mask-wrapper .content-area.barcode .barcode {
  width: 501rpx;
  height: 82rpx;
  margin-top: 22rpx;
}
.mask-wrapper .content-area.barcode .code {
  font-size: 26rpx;
  color: #000;
  margin-top: 42rpx;
}
.mask-wrapper .content-area.barcode .notice {
  font-size: 26rpx;
  color: #666666;
  margin-top: 45rpx;
}
.mask-wrapper .content-area .box {
  width: 100%;
  height: 88rpx;
  border-bottom: 1rpx solid #cacaca;
  line-height: 88rpx;
  text-align: center;
  font-size: 35rpx;
  color: #535758;
}
.mask-wrapper .content-area .box.cancel {
  color: #ea9197;
}
.mask-wrapper .content-area .box.tap {
  background: #f6f6f6;
}
