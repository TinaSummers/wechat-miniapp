/**
* 样式命名规范：
* 容器层级 container > wrapper > area > box > chunk
*/
page {
  width: 100%;
  height: 100%;
  line-height: normal;
  background: #f1f2ec;
  letter-spacing: 2rpx;
}
view,
text,
swiper,
swiper-item,
input,
picker,
textarea,
scroll-view,
rich-text,
image,
form {
  box-sizing: border-box;
  word-break: break-all;
}
::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}
.scroll-view-container {
  width: 100%;
  flex: 1;
  position: relative;
}
scroll-view {
  position: absolute;
  left: 0;
  right: 0;
  top: 40rpx;
  bottom: 40rpx;
}
image {
  display: block;
}
.container {
  width: 100%;
  min-height: 100%;
  font-size: 28rpx;
  color: #1c1d22;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  align-items: center;
  font-family: Arial, Helvetica, sans-serif;
}
radio,
checkbox {
  transform: scale(0.6);
}
.userAvatarUrl {
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
  overflow: hidden;
  display: block;
}
.arrow-ahead {
  /*向前箭头*/
  width: 16rpx;
  height: 16rpx;
  border: 2rpx solid #454545;
  border-bottom: none;
  border-left: none;
  transform: rotate(45deg);
}
.arrow-push {
  /*下推箭头*/
  width: 16rpx;
  height: 16rpx;
  border: 2rpx solid #454545;
  border-bottom: none;
  border-left: none;
  transform: rotate(135deg);
}
.arrow-push-solid {
  /*下推实心箭头*/
  width: 0;
  height: 0;
  border-width: 20rpx 12rpx 0 12rpx;
  border-style: solid;
  border-color: #333 transparent transparent transparent;
}
.hidden-e {
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.hidden-e-r2 {
  /*设置多行文本超出隐藏*/
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
}
.placeholder {
  color: #999;
}
.blank {
  /*占位行*/
  flex: 1;
}
button {
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  padding: 0;
  margin: 0;
  border-radius: 0;
}
button::after {
  border-radius: 0;
  border: none;
}
button.btn-main {
  width: 500rpx;
  height: 80rpx;
  background: #c9192e;
  color: #fff;
  border-radius: 6rpx;
  font-size: 32rpx;
}
.loading-wrapper {
  position: fixed;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  background: #fff;
  z-index: 99;
  color: #999;
}
.loading-wrapper text {
  display: block;
  text-align: center;
}
.zero-wrapper {
  width: 100%;
  padding: 40rpx 0;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.form-wrapper {
  width: 100%;
}
.form-wrapper .profile-area {
  background: #fff;
  padding: 0 40rpx;
}
.form-wrapper .profile-area > view {
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
  border-bottom: 1rpx solid #c8caca;
  position: relative;
}
.form-wrapper .profile-area > view.disabled .name-box,
.form-wrapper .profile-area > view.disabled .element-box {
  color: #ccc;
}
.form-wrapper .profile-area > view.unvalue .element-box {
  color: #999;
}
.form-wrapper .profile-area > view .name-box {
  min-width: 160rpx;
  color: #666666;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
}
.form-wrapper .profile-area > view .name-box .star {
  font-size: 32rpx;
  color: #c61c32;
}
.form-wrapper .profile-area > view .element-box {
  position: relative;
  flex: 1;
  padding-left: 8rpx;
  display: flex;
  flex-direction: row;
  justify-content: flex-end;
  align-items: center;
  min-height: 90rpx;
  color: #333333;
  font-size: 24rpx;
}
.form-wrapper .profile-area > view .element-box input,
.form-wrapper .profile-area > view .element-box picker,
.form-wrapper .profile-area > view .element-box text {
  flex: 1;
  min-height: 90rpx;
  line-height: 90rpx;
  text-align: right;
}
.form-wrapper .profile-area > view .element-box textarea {
  width: 100%;
  height: 120rpx;
  padding: 10rpx;
  margin: 12rpx 0;
  border: 1rpx solid #c8caca;
}
.form-wrapper .profile-area > view .element-box label {
  margin-right: 12rpx;
}
.form-wrapper .profile-area > view .element-box .btn-getPhoneNumber,
.form-wrapper .profile-area > view .element-box .btn-code {
  position: relative;
  width: 150rpx;
  height: 100%;
  background: transparent;
  border: none;
  font-size: 24rpx;
  font-weight: 500;
  color: #5a9cc3;
}
.form-wrapper .profile-area > view .element-box .btn-getPhoneNumber::after,
.form-wrapper .profile-area > view .element-box .btn-code::after {
  border: none;
}
.form-wrapper .profile-area > view .element-box .btn-getPhoneNumber text,
.form-wrapper .profile-area > view .element-box .btn-code text {
  position: absolute;
  left: 6rpx;
  top: 50%;
  width: 2rpx;
  height: 50%;
  transform: translateY(-50%);
  background: #c8caca;
  min-height: auto;
}
.form-wrapper .profile-area > view .arrow-ahead {
  margin-left: 10rpx;
}
.form-wrapper .rule-area {
  padding: 20rpx;
  color: #999;
  font-size: 22rpx;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: flex-start;
}
.form-wrapper .rule-area checkbox {
  transform: scale(0.5);
}
.form-wrapper .rule-area view {
  flex: 1;
}
.form-wrapper .rule-area view text {
  text-decoration: underline;
  color: #5a9cc3;
}
.form-wrapper .hint-area {
  color: #c61c32;
  text-align: center;
  margin: 30rpx 0;
  font-size: 28rpx;
  min-height: 44rpx;
  font-weight: 600;
}
.form-wrapper .btn-main {
  margin: auto;
}
.mask-wrapper {
  position: fixed;
  left: 0;
  top: 0;
  z-index: 1000;
  width: 100%;
  height: 100%;
}
.mask-wrapper .mask-area {
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.6);
}
.mask-wrapper .content-area {
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translateX(-50%) translateY(-50%);
}
.mask-wrapper .content-area .icon-close {
  position: absolute;
  right: 20rpx;
  top: 20rpx;
  width: 50rpx;
  height: 50rpx;
}
.container {
  background: #fff;
}
.container .header-wrapper {
  width: 100%;
  height: 865rpx;
  position: relative;
  overflow: hidden;
  transition: height 0.35s ease-out;
}
.container .header-wrapper.no-member {
  height: 684rpx;
}
.container .header-wrapper.scroll {
  height: 530rpx;
}
.container .header-wrapper image.bg {
  width: 100%;
  height: 865rpx;
}
.container .header-wrapper .area {
  position: absolute;
  width: 100%;
  height: 865rpx;
  top: 50%;
  transform: translateY(-50%);
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.container .header-wrapper .area .avatar-area {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  opacity: 1;
  transition: opacity 0.35s ease-out;
}
.container .header-wrapper .area .avatar-area.scroll {
  opacity: 0;
}
.container .header-wrapper .area .avatar-box {
  display: inline-block;
  width: 140rpx;
  height: 140rpx;
  background: rgba(255, 255, 255, 0.5);
  border: 10rpx solid transparent;
  box-shadow: 0 0 20rpx rgba(0, 0, 0, 0.1);
  border-radius: 50%;
}
.container .header-wrapper .area .nick-box {
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  font-size: 36rpx;
  color: #545759;
  font-weight: bold;
  margin-top: 16rpx;
}
.container .header-wrapper .area .nick-box image.edit {
  width: 24rpx;
  height: 23rpx;
  margin-left: 12rpx;
}
.container .header-wrapper .area .point-box {
  font-size: 28rpx;
  color: #545759;
  margin-top: 20rpx;
}
.container .header-wrapper .area .card-box {
  display: inline-block;
  width: 711rpx;
  height: 426rpx;
  background: #fff;
  border-radius: 15rpx;
  margin-top: 28rpx;
  box-shadow: 0 0 20rpx rgba(51, 51, 51, 0.2);
  position: relative;
  opacity: 1;
  transition: width, margin-top, opacity 0.35s ease-out;
}
.container .header-wrapper .area .card-box.no-member {
  margin-top: 120rpx;
}
.container .header-wrapper .area .card-box.scroll {
  margin-top: 270rpx;
  width: 650rpx;
  opacity: .8;
}
.container .header-wrapper .area .card-box .top-chunk {
  position: absolute;
  width: 100%;
  top: 46rpx;
  padding: 0 40rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: top;
}
.container .header-wrapper .area .card-box .top-chunk .lf image {
  width: 255rpx;
  height: 50rpx;
}
.container .header-wrapper .area .card-box .top-chunk .lf text {
  display: block;
  font-size: 36rpx;
  color: #656766;
  margin-top: 20rpx;
}
.container .header-wrapper .area .card-box .top-chunk .rg image {
  width: 57rpx;
  height: 42rpx;
}
.container .header-wrapper .area .card-box .bottom-chunk {
  position: absolute;
  width: 100%;
  bottom: 59rpx;
  padding: 0 30rpx 0 40rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}
.container .header-wrapper .area .card-box .bottom-chunk .code {
  font-size: 36rpx;
  color: #656766;
}
.container .header-wrapper .area .card-box .bottom-chunk .btn-detail {
  display: inline-block;
  width: 202rpx;
  height: 60rpx;
  background: #f68b92;
  line-height: 60rpx;
  text-align: center;
  font-size: 29rpx;
  color: #fff;
  font-weight: bold;
}
.container .header-wrapper .area .card-box .bottom-chunk .btn-detail.tap {
  background: #e86971;
}
.container .header-wrapper .area .card-box .content-chunk {
  position: absolute;
  top: 0;
  bottom: 0;
  left: 38rpx;
  right: 38rpx;
  display: flex;
  flex-direction: column;
  justify-content: top;
  align-items: center;
}
.container .header-wrapper .area .card-box .content-chunk .logo {
  width: 360rpx;
  height: 69rpx;
  margin-top: 100rpx;
}
.container .header-wrapper .area .card-box .content-chunk .line {
  width: 100%;
  height: 1rpx;
  background: #cdcdcd;
  margin-top: 86rpx;
}
.container .header-wrapper .area .card-box .content-chunk .btn-receive {
  margin-top: 46rpx;
  width: 251rpx;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  font-size: 29rpx;
  color: #fff;
  font-weight: bold;
  background: #f68b92;
}
.container .header-wrapper .area .card-box .content-chunk .btn-receive.tap {
  background: #e86971;
}
.container .model-wrapper {
  width: 100%;
  background: #fff;
  border-top: 10rpx solid #f1f2ed;
}
.container .model-wrapper:first-child {
  border-top: 0;
}
.container .model-wrapper .title-area {
  width: 100%;
  height: 80rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}
.container .model-wrapper .title-area .lf {
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
}
.container .model-wrapper .title-area .lf .icon {
  display: inline-block;
  width: 20rpx;
  height: 35rpx;
  border-right: 8rpx solid #fcc9c7;
  background: #f19499;
}
.container .model-wrapper .title-area .lf text {
  font-size: 30rpx;
  color: #545759;
  margin-left: 20rpx;
}
.container .model-wrapper .title-area .rg {
  display: flex;
  flex-direction: row;
  justify-content: flex-end;
  align-items: center;
}
.container .model-wrapper .title-area .rg text {
  margin-right: 10rpx;
  font-size: 26rpx;
  color: #545759;
}
.container .model-wrapper .title-area .rg .arrow-right {
  width: 17rpx;
  height: 17rpx;
  border-right: 6rpx solid #cbcccd;
  border-bottom: 6rpx solid #cbcccd;
  transform: rotate(-45deg);
  margin-right: 40rpx;
}
.container .model-wrapper .content-area {
  width: 100%;
  padding: 0 20rpx;
  position: relative;
}
.container .model-wrapper .content-area::before {
  content: '';
  width: 710rpx;
  height: 1rpx;
  background: #e5e6e6;
  position: absolute;
  top: 0;
  left: 20rpx;
}
.container .model-wrapper .content-area .order-box {
  width: 100%;
  height: 170rpx;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}
.container .model-wrapper .content-area .order-box .chunk {
  flex: 1;
  position: relative;
  text-align: center;
}
.container .model-wrapper .content-area .order-box .chunk.tap {
  background: #f6f6f6;
}
.container .model-wrapper .content-area .order-box .chunk image {
  width: 70rpx;
  height: 50rpx;
  display: inline-block;
}
.container .model-wrapper .content-area .order-box .chunk text {
  display: block;
  font-size: 26rpx;
  color: #545759;
  margin-top: 24rpx;
}
.container .model-wrapper .content-area .order-box .chunk .icon {
  display: inline-block;
  width: 36rpx;
  height: 36rpx;
  border-radius: 50%;
  background: #f68b92;
  position: absolute;
  top: -15rpx;
  right: 36rpx;
  color: #fff;
  line-height: 36rpx;
  text-align: center;
  font-size: 20rpx;
  font-weight: bold;
}
.container .model-wrapper .content-area .member-wrapper {
  width: 100%;
}
.container .model-wrapper .content-area .member-wrapper .area {
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  border-top: 1rpx solid #e5e6e6;
}
.container .model-wrapper .content-area .member-wrapper .area .chunk {
  flex: 1;
  height: 162rpx;
  border-right: 1rpx solid #e5e6e6;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.container .model-wrapper .content-area .member-wrapper .area .chunk.tap {
  background: #f6f6f6;
}
.container .model-wrapper .content-area .member-wrapper .area .chunk:last-child {
  border-right: 0;
}
.container .model-wrapper .content-area .member-wrapper .area .chunk image {
  width: 60rpx;
  height: 60rpx;
}
.container .model-wrapper .content-area .member-wrapper .area .chunk text {
  font-size: 28rpx;
  color: #545759;
  margin-top: 18rpx;
}
.container .model-wrapper .content-area .center-wrapper {
  width: 100%;
}
.container .model-wrapper .content-area .center-wrapper .area {
  width: 100%;
  height: 89rpx;
  padding: 0 20rpx;
  border-bottom: 1rpx solid #e5e6e6;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}
.container .model-wrapper .content-area .center-wrapper .area.tap {
  background: #f6f6f6;
}
.container .model-wrapper .content-area .center-wrapper .area .chunk {
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
}
.container .model-wrapper .content-area .center-wrapper .area .chunk image {
  width: 54rpx;
  height: 50rpx;
}
.container .model-wrapper .content-area .center-wrapper .area .chunk text {
  font-size: 30rpx;
  color: #545759;
  margin-left: 14rpx;
}
.container .model-wrapper .content-area .center-wrapper .area .sub-title {
  font-size: 26rpx;
  color: #999999;
}
.container .bottom-banner-wrapper {
  padding: 22rpx 0 30rpx 0;
}
.container .bottom-banner-wrapper image {
  align-self: center;
  width: 710rpx;
  height: 300rpx;
}
